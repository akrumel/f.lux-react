{
  presets: ['es2015', 'react'],
  plugins: [
    "transform-decorators-legacy",
    ["transform-es2015-spread", { "loose": true }],
    "transform-object-rest-spread"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }],
        ["transform-class-properties", { "spec": true }]
      ]
    }
  }
}
